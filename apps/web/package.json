{
  "name": "@riscovery/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "prisma generate --schema=../api/prisma/schema.prisma && next build",
    "postinstall": "prisma generate --schema=../api/prisma/schema.prisma",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^5.20.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "next-intl": "^3.7.0",
    "prisma": "^5.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "stylis": "^4.3.1",
    "stylis-plugin-rtl": "^2.1.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@riscovery/eslint-config": "workspace:*",
    "@riscovery/typescript-config": "workspace:*",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/stylis": "^4.2.7",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
